{
    "name": "gollumsf/rest-bundle",
    "type": "symfony-bundle",
    "description": "Very simple REST Api implementation",
    "keywords" : [
        "api",
        "simple",
        "rest",
        "gollumsf"
    ],
    "homepage": "https://github.com/GollumSF/rest-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Damien Duboeuf",
            "email": "smeagolworms4@gmail.com"
        }
    ],
    "support" : {
        "email": "smeagolworms4@gmail.com",
        "wiki": "https://github.com/GollumSF/rest-bundle/blob/master/README.md"
    },
    "require" : {
        "symfony/validator": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/event-dispatcher": "^5.0",
        "symfony/http-foundation": "^5.0",
        "symfony/http-kernel": "^5.0",
        "doctrine/orm": "^2.7",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/config": "^5.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0",
        "phpunit/phpunit": "^8.5",
        "gollumsf/reflection-property-test": "^1.0"
    },
    "autoload" : {
        "psr-4" : {
            "GollumSF\\RestBundle\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\GollumSF\\RestBundle\\" : "tests/"
        }
    }
}
