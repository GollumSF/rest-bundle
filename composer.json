{
    "name": "gollumsf/rest-bundle",
    "type": "symfony-bundle",
    "description": "Very simple REST Api implementation",
    "keywords" : [
        "api",
        "simple",
        "rest",
        "gollumsf"
    ],
    "homepage": "https://github.com/GollumSF/rest-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Damien Duboeuf",
            "email": "smeagolworms4@gmail.com"
        }
    ],
    "support" : {
        "email": "smeagolworms4@gmail.com",
        "wiki": "https://github.com/GollumSF/rest-bundle/blob/master/README.md"
    },
    "require" : {
        "php": "^7.2",
        "gollumsf/enum": "^1.0",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/config": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/event-dispatcher": "^4.4|^5.0",
        "symfony/http-foundation": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/property-access": "^4.4|^5.0",
        "symfony/property-info": "^4.4|^5.0",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.4|^5.0",
        "symfony/serializer": "^4.4|^5.0",
        "symfony/validator": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "v3.3|^3.3",
        "gollumsf/reflection-property-test": "^1.0",
        "phpunit/phpunit": "^8.5",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "nyholm/symfony-bundle-test": "^1.6",
        "symfony/browser-kit": "^4.4|^5.0",
        "symfony/proxy-manager-bridge": "^4.4|^5.0",
        "gollumsf/entity-relation-setter": "^2.0"
    },
    "autoload" : {
        "psr-4" : {
            "GollumSF\\RestBundle\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\GollumSF\\RestBundle\\" : "tests/"
        }
    }
}
